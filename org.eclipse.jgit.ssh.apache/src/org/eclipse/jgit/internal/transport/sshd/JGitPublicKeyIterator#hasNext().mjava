	@Override
	public boolean hasNext() {
		if (!isOpen()) {
			return false;
		}
		if (hasElement != null) {
			return hasElement.booleanValue();
		}
		while (current == null || !current.hasNext()) {
			if (keyIter.hasNext()) {
				current = keyIter.next();
			} else {
				current = null;
				hasElement = Boolean.FALSE;
				return false;
			}
		}
		hasElement = Boolean.TRUE;
		return true;
	}

