		public String toString() {
			final StringBuilder r = new StringBuilder();
			r.append("FAST("); //$NON-NLS-1$
			for (int i = 0; i < paths.length; i++) {
				if (i > 0)
					r.append(" OR "); //$NON-NLS-1$
				r.append(paths[i].toString());
			}
			r.append(")"); //$NON-NLS-1$
			return r.toString();
		}

