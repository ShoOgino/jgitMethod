		public void run() {
			InputStream is = p.getErrorStream();
			try {
				int ch;
				while ((ch = is.read()) != -1) {
					System.err.print((char) ch);
				}
			} catch (IOException e) {
				logError(e);
				fail.set(true);
			}
			try {
				is.close();
			} catch (IOException e) {
				logError(e);
				fail.set(true);
			}
		}

