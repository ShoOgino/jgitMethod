		private void diff(Edit r, long[] pCommon, int pIdx, int pEnd) {
			switch (r.getType()) {
			case INSERT:
			case DELETE:
				edits.add(r);
				break;

			case REPLACE:
				diffReplace(r, pCommon, pIdx, pEnd);
				break;

			case EMPTY:
				break;

			default:
				throw new IllegalStateException();
			}
		}

