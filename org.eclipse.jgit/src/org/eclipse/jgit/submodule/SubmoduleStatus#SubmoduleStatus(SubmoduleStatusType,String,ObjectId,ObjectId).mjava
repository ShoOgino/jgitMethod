	/**
	 * Create submodule status
	 *
	 * @param type
	 * @param path
	 * @param indexId
	 * @param headId
	 */
	public SubmoduleStatus(final SubmoduleStatusType type, final String path,
			final ObjectId indexId, final ObjectId headId) {
		this.type = type;
		this.path = path;
		this.indexId = indexId;
		this.headId = headId;
	}

