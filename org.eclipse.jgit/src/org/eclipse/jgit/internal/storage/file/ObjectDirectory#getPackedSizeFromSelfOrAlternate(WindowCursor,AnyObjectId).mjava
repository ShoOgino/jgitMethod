	private long getPackedSizeFromSelfOrAlternate(WindowCursor curs,
			AnyObjectId id) {
		long len = getPackedObjectSize(curs, id);
		if (0 <= len)
			return len;
		for (AlternateHandle alt : myAlternates()) {
			len = alt.db.getPackedSizeFromSelfOrAlternate(curs, id);
			if (0 <= len)
				return len;
		}
		return -1;
	}

