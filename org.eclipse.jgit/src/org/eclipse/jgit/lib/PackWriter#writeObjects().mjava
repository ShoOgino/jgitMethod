	private void writeObjects() throws IOException {
		for (List<ObjectToPack> list : objectsLists) {
			for (ObjectToPack otp : list) {
				if (writeMonitor.isCancelled())
					throw new IOException(
							"Packing cancelled during objects writing");
				if (!otp.isWritten())
					writeObject(otp);
			}
		}
	}

