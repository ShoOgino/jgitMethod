	/**
	 * Construct a walker for the index and one tree.
	 *
	 * @param index
	 * @param tree
	 * @param root
	 * @param visitor
	 */
	public IndexTreeWalker(GitIndex index, Tree tree, File root, IndexTreeVisitor visitor) {
		this.mainTree = tree;
		this.root = root;
		this.visitor = visitor;
		this.newTree = null;

		threeTrees = false;

		indexMembers = index.getMembers();
	}

