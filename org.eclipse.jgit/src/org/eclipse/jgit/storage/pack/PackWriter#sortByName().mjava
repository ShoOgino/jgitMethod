	private List<ObjectToPack> sortByName() {
		if (sortedByName == null) {
			int cnt = 0;
			for (List<ObjectToPack> list : objectsLists)
				cnt += list.size();
			sortedByName = new BlockList<ObjectToPack>(cnt);
			for (List<ObjectToPack> list : objectsLists)
				sortedByName.addAll(list);
			Collections.sort(sortedByName);
		}
		return sortedByName;
	}

