		public int compare(Ref o1, Ref o2) {
			try {
				Object obj1 = getRepository().mapObject(o1.getObjectId(), o1.getName());
				Object obj2 = getRepository().mapObject(o2.getObjectId(), o2.getName());
				long t1 = timeof(obj1);
				long t2 = timeof(obj2);
				if (t1 > t2)
					return -1;
				if (t1 < t2)
					return 1;
				return 0;
			} catch (IOException e) {
				// ignore
				return 0;
			}
		}

