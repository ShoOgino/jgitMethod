	private void want(RevObject o) throws MissingObjectException, IOException {
		if (!o.has(WANT)) {
			o.add(WANT);
			wantAll.add(o);

			if (o instanceof RevCommit)
				wantCommits.add((RevCommit) o);

			else if (o instanceof RevTag) {
				o = walk.peel(o);
				if (o instanceof RevCommit)
					want(o);
			}
		}
	}

