	/**
	 * Assert booleans are equal
	 *
	 * @param message
	 *            message
	 * @param expect
	 *            expected value
	 * @param actual
	 *            actual value
	 */
	public static void assertEquals(String message, boolean expect,
			boolean actual) {
		org.junit.Assert
				.assertEquals(message, valueOf(expect), valueOf(actual));
	}

