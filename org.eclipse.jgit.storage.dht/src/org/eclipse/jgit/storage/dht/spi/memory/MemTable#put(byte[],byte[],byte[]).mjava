	/**
	 * Put a value into a cell.
	 *
	 * @param row
	 * @param col
	 * @param val
	 */
	public void put(byte[] row, byte[] col, byte[] val) {
		synchronized (lock) {
			Key rowKey = new Key(row);
			Map<Key, Cell> r = map.get(rowKey);
			if (r == null) {
				r = new HashMap<Key, Cell>(4);
				map.put(rowKey, r);
			}
			r.put(new Key(col), new Cell(row, col, val));
		}
	}

