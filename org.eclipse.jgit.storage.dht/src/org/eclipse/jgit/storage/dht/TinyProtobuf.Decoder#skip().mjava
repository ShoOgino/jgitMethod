		/** Skip the current field's value. */
		public void skip() {
			switch (type) {
			case WIRE_VARINT:
				varint64();
				break;
			case WIRE_FIXED_64:
				ptr += 8;
				break;
			case WIRE_LEN_DELIM:
				ptr += varint32();
				break;
			case WIRE_FIXED_32:
				ptr += 4;
				break;
			default:
				throw new IllegalStateException(MessageFormat.format(DhtText
						.get().protobufUnsupportedFieldType, Integer
						.valueOf(type)));
			}
		}

