	private String randomLine(int len) {
		final int a = 97; // 'a'
		int z = 122; // 'z'
		Random random = new Random();
		StringBuilder buffer = new StringBuilder(len);
		for (int i = 0; i < len; i++) {
			int rnd = a + (int) (random.nextFloat() * (z - a + 1));
			buffer.append((char) rnd);
		}
		buffer.append('\n');
		return buffer.toString();
	}

