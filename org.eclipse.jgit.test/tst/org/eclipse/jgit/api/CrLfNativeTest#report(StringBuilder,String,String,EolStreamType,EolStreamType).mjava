	private void report(StringBuilder result, String prefix, String label,
			EolStreamType a,
			EolStreamType b) {
		if (a == null || b == null || !a.equals(b)) {
			result.append(prefix).append(' ').append(label).append(": ")
					.append(toString(a)).append(" != ").append(toString(b))
					.append('\n');
		}
	}

