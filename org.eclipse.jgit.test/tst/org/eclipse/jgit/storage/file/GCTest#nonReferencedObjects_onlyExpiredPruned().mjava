	@Test
	public void nonReferencedObjects_onlyExpiredPruned() throws Exception {
		RevBlob a = tr.blob("a");

		fsTick();
		long start = now();

		fsTick();
		RevBlob b = tr.blob("b");
		gc.setExpireAgeMillis(now() - start);
		gc.prune(Collections.<ObjectId> emptySet());
		assertFalse(repo.hasObject(a));
		assertTrue(repo.hasObject(b));
	}

