	@Test
	public void testCaret() throws Exception {
		assertMatch("^", "^", true);
		assertMatch("^x", "^x", true);
		assertMatch("^x", "x^", false);
		assertMatch("^x", "^", false);

		assertMatch("^x.*", "^x.a", true);
		assertMatch("*^", "x^", true);
		assertMatch("*.^", "x.^", true);

		assertMatch("x*^", "xa^", true);
		assertMatch("^*x", "^ax", true);
		assertMatch("^*x", "ax", false);
		assertMatch("[a^b]", "^", true);
	}

