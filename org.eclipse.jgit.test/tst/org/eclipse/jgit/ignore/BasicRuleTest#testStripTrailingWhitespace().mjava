	@Test
	public void testStripTrailingWhitespace() {
		assertEquals("", Strings.stripTrailingWhitespace(""));
		assertEquals("", Strings.stripTrailingWhitespace("   "));
		assertEquals("a", Strings.stripTrailingWhitespace("a"));
		assertEquals("a", Strings.stripTrailingWhitespace("a "));
		assertEquals("a", Strings.stripTrailingWhitespace("a  "));
		assertEquals("a", Strings.stripTrailingWhitespace("a \t"));
	}

