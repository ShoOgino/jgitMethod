	public void testQuote_User() {
		assertEquals("~foo/", BOURNE_USER_PATH.quote("~foo"));
		assertEquals("~foo/", BOURNE_USER_PATH.quote("~foo/"));
		assertEquals("~/", BOURNE_USER_PATH.quote("~/"));

		assertEquals("~foo/'a'", BOURNE_USER_PATH.quote("~foo/a"));
		assertEquals("~/'a'", BOURNE_USER_PATH.quote("~/a"));
	}

