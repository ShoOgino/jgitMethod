	@Override
	protected void run() throws Exception {
		final int cnt = 100;
		final long start = System.currentTimeMillis();
		for (int i = 0; i < cnt; i++)
			DirCache.read(db);
		final long end = System.currentTimeMillis();
		out.print(" ");
		out.println(MessageFormat.format(CLIText.get().averageMSPerRead, (end - start) / cnt));
	}

