	@Override
	protected void show(final RevCommit c) throws Exception {
		if (c.has(RevFlag.UNINTERESTING))
			out.print('-');
		c.getId().copyTo(outbuffer, out);
		if (parents)
			for (int i = 0; i < c.getParentCount(); i++) {
				out.print(' ');
				c.getParent(i).getId().copyTo(outbuffer, out);
			}
		out.println();
	}

