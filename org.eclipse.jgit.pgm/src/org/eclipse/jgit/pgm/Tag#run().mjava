	@Override
	protected void run() throws Exception {
		Git git = new Git(db);
		TagCommand command = git.tag().setForceUpdate(force).setMessage(message).setName(tagName);

		if (object != null) {
			RevWalk walk = new RevWalk(db);
			command.setObjectId(walk.parseAny(object));
		}

		command.call();
	}

