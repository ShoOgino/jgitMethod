	private boolean get(URIish uri, CredentialItem.StringType item) {
		if (item.isValueSecure()) {
			char[] v = cons.readPassword("%s: ", item.getPromptText());
			if (v != null) {
				item.setValue(new String(v));
				return true;
			} else {
				return false;
			}
		} else {
			String v = cons.readLine("%s: ", item.getPromptText());
			if (v != null) {
				item.setValue(v);
				return true;
			} else {
				return false;
			}
		}
	}

